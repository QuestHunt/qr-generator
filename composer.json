{
    "name": "questhunt/qr_generator",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Peter Gnodde",
            "email": "peter@gnodde.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/flex": "^2.2",
        "symfony/console": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/runtime": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/yaml": "^6.1",
        "tecnickcom/tcpdf": "^6.4",
        "phpoffice/phpspreadsheet": "^1.24"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
